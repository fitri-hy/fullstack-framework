{
  "name": "my-fullstack-framework",
  "version": "1.0.0",
  "scripts": {
    "build": "cd frontend && npx webpack --config webpack.config.js",
    "start": "npm run build && node backend/server.js",
    "dev": "concurrently \"npm run build-frontend\" \"npm run start-backend\"",
    "build-frontend": "cd frontend && npx webpack --config webpack.config.js --watch",
    "start-backend": "nodemon backend/server.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-react": "^7.22.0",
    "babel-loader": "^9.1.2",
    "concurrently": "^7.1.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.8.1",
    "file-loader": "^6.2.0",
    "fs": "^0.0.1-security",
    "html-webpack-plugin": "^5.6.0",
    "nodemon": "^3.1.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.76.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.2"
  }
}
